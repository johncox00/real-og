{
  "name": "app",
  "private": true,
  "devDependencies": {
    "esbuild": "^0.27.0"
  },
  "scripts": {
    "build": "esbuild app/javascript/*.* --bundle --sourcemap --format=esm --outdir=app/assets/builds --loader:.js=jsx --loader:.jsx=jsx --public-path=/assets",
    "watch": "esbuild app/javascript/application.js --bundle --sourcemap --outdir=app/assets/builds --loader:.js=jsx --loader:.jsx=jsx --watch"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@hotwired/turbo-rails": "^8.0.20",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
